<tr id="<%= dom_id row %>" <%= 'class=deleted' if row.status && row.status < 1 %>>
  <td><%= row.permalink %></td>
  <td><%= row.title %></td>
  <td><%= row.updated_at %></td>
  <td class="actions">
    <%= link_to_edit row %> |
    <%= link_to_hide row, 'data-confirm' => 'Are you sure to hide/show this Page?' %> |
    <%= link_to 'Photo', page_photos_path(row), :target => 'edit-win' %>
  </td>
</tr>
