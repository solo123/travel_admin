  <tr id="<%= dom_id row %>" <%= 'class=\'deleted\'' if !row.status || row.status < 1 %>>
      <td><%= row.id %></td>
      <td><%= row.short_name %></td>
      <td><%= row.company_name %></td>
      <td><%= row.company_type %></td>
      <td><%= row.icon_url %></td>
      <td><%= row.website %></td>
      <td><%= row.status %></td>
    <td class="actions">
      <%= link_to_edit row %> |
      <%= link_to 'Photos', company_photos_path(row), {:target => 'edit-win'} %> |
      <%= link_to_hide row, 'data-confirm' => 'Are you sure to hide/show this Agent?' %>
    </td>
  </tr>
