<%= form_for( @object, :remote => true) do |f| %>
  <div class="container">
  	<div class='span-8'>
	  <%= f.field_container :key do %>
	  	<%= f.label :key %><br />
	  	<%= f.text_field :key %>
    <% end %>
	  <%= f.field_container :title do %>
	  	<%= f.label :title %><br />
	  	<%= f.text_field :title %>
    <% end %>
	  <%= f.field_container :val do %>
	  	<%= f.label :val %><br />
	  	<%= f.text_field :val %>
    <% end %>
    Created at: <%= @object.created_at %><br />
    Updated at: <%= @object.updated_at %><br />
	  </div>
	  <div class='span-8 last'>
    </div>   
	</div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>



