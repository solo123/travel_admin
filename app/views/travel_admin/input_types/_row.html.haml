<tr id="#{dom_id row}" #{'class=\'deleted\'' if !row.status || row.status < 1}>
%td.cell-middle= row.id
%td= row.type_name
%td= row.type_text
%td= row.type_value
%td= row.status
%td= row.created_at
%td= row.updated_at
%td.cell-middle
  .btn-group
    = row_link_to_edit row
    = row_link_to_hide row
