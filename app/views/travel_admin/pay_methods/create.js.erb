<% if flash[:error] %>
  alert("<%= raw flash[:error] %>");
  <% flash.discard %>
<% else %>
  
  $("#list_payments")
    .html("<%= j render(:partial => 'travel_admin/payments/payments', :object => @object.payment.payment_data) %>");
  to_localtime();
  $('#edit_payment_div').dialog('close');

<% end %>



