<% if flash[:error] %>
  alert("<%= raw flash[:error] %>");
  <% flash.discard %>
<% else %>
  <% if @order.status == 3 %>
    location.reload(true);
  <% else %>
  $('#edit_payment_div').modal('hide');
  $("#payments")
	.html("<%= j render(:partial => 'travel_admin/payments/payments', :object => @order) %>")
	.hide().slideDown();
//  to_localtime();
  <% end %>
<% end %>


