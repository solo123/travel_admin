<% if flash[:error] %>
  alert("<%= flash[:error] %>");
  <% flash.discard %>
<% else %>
  $("#edit_order_div").modal('hide');
  if ($('form.edit_schedule_assignment').length>0){
    $("#assignment_tab")
      .replaceWith("<%=j render(:partial => 'travel_admin/schedule_assignments/seats', :locals => { :schedule => @object.schedule, :assignment => @object.schedule_assignment}) %>");
    $('#unseat_orders').html("<%=j render 'travel_admin/schedules/orders', :orders => @object.schedule.orders %>")
  } else
  	  location.reload(true);
<% end %>


