    <tr>
        <td><%= link_to row.order_number, order_path(row) %></td>
        <td><%= row.order_detail.full_name if row.order_detail %></td>
        <td><%= row.schedule.title if row.schedule %></td>
        <td><%= time_tag row.created_at %></td>
        <td><%= type_text.get_text('order.status', row.status) %></td>
      <td class='actions'>
          <%= row_link_to_edit row %>
      </td>
    </tr>
